{
    "configurations": [
        {
            "name": "Wine",
            "includePath": [
                "~/Documents/programming/builds/wine/x64/include/**",
                "${workspaceFolder}/include/**",
                "/usr/lib/gcc/x86_64-w64-mingw32/11.2.0/include/**",
                "/usr/include"
            ],
            "defines": ["__WINESRC__"],
            "compilerPath": "/usr/bin/x86_64-w64-mingw32-gcc",
            "cStandard": "c89",
            "cppStandard": "c++14",
            "intelliSenseMode": "windows-gcc-x64"
            
        }
    ],
    "version": 4
}
